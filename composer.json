{
    "name": "microsoft/azure-sdk",
    "description": "Azure SDK for PHP",
    "type": "library",
    "license": "Apache-2.0",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Microsoft\\Rest\\": "src/Microsoft/Rest/",
            "Microsoft\\Azure\\": "gen/Microsoft/Azure/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Microsoft\\Azure\\": "tests/Microsoft/Azure/",
            "Microsoft\\Rest\\": "tests/Microsoft/Rest/"
        }
    },
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    }
}
